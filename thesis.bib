@article {eickermann2005steering,
	author = {Eickermann, Thomas and Frings, Wolfgang and Gibbon, Paul and Kirtchakova, Lidia and Mallmann, Daniel and Visser, Anke},
	title = {Steering UNICORE applications with VISIT},
	volume = {363},
	number = {1833},
	pages = {1855--1865},
	year = {2005},
	doi = {10.1098/rsta.2005.1615},
	publisher = {The Royal Society},
	abstract = {Thomas Eickermann, Wolfgang Frings, Paul Gibbon, Lidia Kirtchakova, Daniel Mallmann and Anke VisserForschungszentrum J{\"u}lich GmbH and John von Neumann, Institute for Computing52425 J{\"u}lich, GermanyAbstractThe UNICORE (UNiform Interface to COmputing REsources) software provides a Grid infrastructure together with a computing portal for engineers and scientists to access supercomputer centres from anywhere on the Internet. While UNICORE is primarily designed for the submission and control of batch jobs, it is also feasible to establish an on-line connection between an application and the UNICORE user-client. This opens up the possibility of performing on-line visualization and computational steering of applications under UNICORE control while maintaining the security provided by this system. This contribution describes the design of a steering extension to UNICORE based on the steering toolkit VISIT (VISualization Interface Toolkit). VISIT is a lightweight library that supports bidirectional data exchange between visualizations and parallel applications. As an example application, a parallel simulation of a laser-plasma interaction that can be steered by an AVS/Express application is presented.Keywords: computational steeringvisualizationGrid computingFootnotesOne contribution of 27 to a Theme {\textquoteleft}Scientific Grid computing{\textquoteright}.↵www.unicore.de.↵www.eurogrid.org.↵www.globus.org.↵www.deisa.org.↵www.accessgrid.org.↵www.fz-juelich.de/zam/visit.↵www.unigrids.org.{\textcopyright} 2005 The Royal Society},
	isbn = {1471-2962},
	issn = {1364-503X},
	journal = {Philosophical Transactions of the Royal Society of London A: Mathematical, Physical and Engineering Sciences}
}

